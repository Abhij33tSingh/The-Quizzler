);\n  }, []);\n\n  return (\n    <div>\n      <header>\n        <h1 onClick={() => navigate(\"/homepage/\" + quiz.userId)}>Quizzler</h1>\n\n        <div className=\"ProfilenavbarItems\">\n          <nav onClick={() => navigate(\"/createquiz/\" + quiz.userId)}>\n            Create a Quiz\n          </nav>\n          <nav onClick={() => navigate(\"/AboutUs/\" + quiz.userId)}>About us</nav>\n        </div>\n      </header>\n      <div>\n      \n      <div className=\"QuizContent\">\n      <h1>Success!!! Quiz Created</h1>\n        { quiz.allQuestions &&\n\n          (<table id=\"Results\">\n            <tbody>\n                  <tr>\n                  <td>\n                      <h3>s/no</h3>\n                  </td>\n                  <td>\n                      <h3>Question</h3>\n                  </td>\n                  <td>\n                      <h3>Answer</h3>\n                  </td>\n                  <td>Options</td>\n                  </tr>\n            {quiz.allQuestions.map((q,key)=>\n              <tr key={key}>\n                <td>{key + 1}</td>\n              <td>{q.Question}</td>\n              <td>{q.Answer}</td>\n              <td>{q.option1},{q.option2},{q.option3},{q.option4}</td>\n            </tr>)}\n          </tbody>\n          </table>)\n        }\n        <button \n        onClick={()=>{\n          navigate(\"/homepage/\" + quiz.userId)\n        }}\n        >Go To Home</button>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default QuizCreated;\n","/home/shivam/new/The-Quizzler/ClientSide/src/components/LoginPage.jsx",[],"/home/shivam/new/The-Quizzler/ClientSide/src/components/Profile.jsx",["85","86"],"/home/shivam/new/The-Quizzler/ClientSide/src/components/AttemptQuiz.jsx",["87","88","89","90","91","92"],"/home/shivam/new/The-Quizzler/ClientSide/src/components/HomePage.jsx",["93","94","95","96","97","98"],"/home/shivam/new/The-Quizzler/ClientSide/src/components/CreateQuizPage.jsx",["99","100","101","102"],"/home/shivam/new/Th